{
    "name": "@gitbook/expr",
    "description": "Safely evaluate & parse user-defined GitBook expressions.",
    "version": "1.2.4",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "sideEffects": false,
    "dependencies": {
        "acorn": "^8.14.0",
        "acorn-loose": "8.4.0",
        "acorn-walk": "^8.3.4",
        "assert-never": "catalog:",
        "escodegen": "^2.1.0",
        "eval-estree-expression": "github:jonschlinkert/eval-estree-expression#fb0246a"
    },
    "devDependencies": {
        "@tsconfig/strictest": "catalog:",
        "@tsconfig/node20": "catalog:",
        "@types/escodegen": "^0.0.10",
        "@types/estree": "^1.0.6",
        "@types/json-schema": "^7.0.15",
        "bun-types": "catalog:",
        "tsdown": "catalog:",
        "typescript": "catalog:"
    },
    "scripts": {
        "build": "tsdown",
        "typecheck": "tsc --noEmit",
        "unit": "bun test",
        "clean": "rm -rf ./dist",
        "dev": "bun run build -- --watch ./src",
        "publish-to-npm": "../../scripts/publish-if-new.sh"
    },
    "files": ["dist", "README.md", "CHANGELOG.md"],
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "url": "https://github.com/GitbookIO/gitbook"
    }
}
