{
    "name": "@gitbook/react-math",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "development": "./src/index.ts",
            "default": "./dist/index.js"
        }
    },
    "version": "0.6.4",
    "sideEffects": false,
    "dependencies": {
        "katex": "^0.16.25",
        "mathjax": "^3.2.2",
        "object-hash": "^3.0.0"
    },
    "peerDependencies": {
        "react": "*"
    },
    "devDependencies": {
        "@types/katex": "^0.16.7",
        "@types/react": "catalog:",
        "bun-types": "catalog:",
        "react": "catalog:",
        "tsdown": "catalog:",
        "typescript": "catalog:"
    },
    "scripts": {
        "build": "tsdown",
        "typecheck": "tsc --noEmit",
        "dev": "bun run build -- --watch ./src",
        "clean": "rm -rf ./dist",
        "publish-to-npm": "../../scripts/publish-if-new.sh"
    },
    "bin": {
        "gitbook-math": "./bin/gitbook-math.js"
    },
    "files": ["dist", "bin", "css", "README.md", "CHANGELOG.md"],
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "url": "https://github.com/GitbookIO/gitbook"
    }
}
